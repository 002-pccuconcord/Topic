mysql -u root -p Aa1234
use today

CREATE OR REPLACE EVENT Time
ON SCHEDULE EVERY 1 MINUTE 
STARTS CURRENT_TIMESTAMP
ENDS CURRENT_TIMESTAMP + INTERVAL 1 MONTH 
ENABLE
DO INSERT INTO history.HDD (SELECT * FROM today.RAM where num not in (SELECT num FROM history.HDD));

ALTER EVENT Time ON COMPLETION PRESERVE ENABLE;
